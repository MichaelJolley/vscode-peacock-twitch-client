{
	"name": "vscode-peacock-twitch-client",
	"displayName": "vscode-peacock-twitch-client",
	"description": "Peacock VSCode Twitch Client",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42",
		"@types/dotenv": "~4.0.3",
		"@types/tmi.js": "~1.4.0"
	},
	"dependencies": {
		"dotenv": "~7.0.0",
		"tmi.js": "~1.4.2"
	}
}
